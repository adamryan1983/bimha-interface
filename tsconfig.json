{
  "compilerOptions": {
    "target": "es6",
    "lib": [
      "dom",
      "dom.iterable",
      "esnext"
    ],
    "allowJs": true,
    "skipLibCheck": true,
    "strict": true,
    "forceConsistentCasingInFileNames": true,
    "noEmit": true,
    "esModuleInterop": true,
    "module": "esnext",
    "moduleResolution": "node",
    "resolveJsonModule": true,
    "isolatedModules": true,
    "jsx": "preserve",
    "baseUrl": ".",
    "paths": {
      "@components/*": [
        "components/*"
      ],
      "@styles/*": [
        "styles/*"
      ],
      "@public/*": [
        "public/*"
      ],
      "@pages/*": [
        "pages/*"
      ],
      "@lib/*": [
        "lib/*"
      ],
      "@utils/*": [
        "utils/*"
      ],
      "@context/*" : [
        "lib/context/*"
      ]
    },
    "incremental": true
  },
  "include": [
    "next-env.d.ts",
    "**/*.ts",
    "**/*.tsx",
    "pages/roster/Roster.jsxpages/api/schedule/Calendar.jsx.jsxpages/api/schedule/Schedule.jsx.jsxngodbold.js",
    "pages/index.tsx",
    "pages/index.tsx",
    "utils/harperdb.js",
    "pages/roster/Roster.jsx"
, "lib/context/state.jsx", "pages/divisions/Division.jsx", "pages/divisions/Division.jsx", "pages/MainPage.jsx", "pages/MainPage.jsx"  ],
  "exclude": [
    "node_modules"
  ]
}
